{
    "manifest_version": 2,
    "name": "colourful",
    "version": "1.0",
    "description": "kaleidoscope for the everyday browser",
    "content_scripts": [
        {
          "matches": ["https://www.google.com/*"],
          "js": ["jquery.min.js", "./main.js"]
        }
      ],
    "browser_action": {
    "default_icon" : "c-logo.png",
    "default_popup": "popup.html",
    "default_title": "My First Chrome Extension"
    },
    "permissions": [
      "https://www.google.com/*",
      "tabs"]
}



