{
    "manifest_version": 2,
    "name": "colourful",
    "version": "1.0",
    "description": "kaleidoscope for the everyday browser",
    "content_scripts": [
        {
          "matches": ["https://*/*", "http://*/*"],
          "js": ["jquery.min.js", "./main.js"]
        }
      ],
    "browser_action": {
    "default_icon" : "c-5logo.png",
    "default_popup": "popup.html",
    "default_title": "Colourful"
    },
    "permissions": ["https://*/*", "http://*/*",
      "tabs"]
}



